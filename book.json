{
  "title": "Mathwiki",
  "author": "Casper Schorr",
  "description": "This is the mathwiki.",
  "language": "en",
  "gitbook": ">=3.0.0",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "disqus",
    "github",
    "mermaid",
    "sharing",
    "search",
    "mathjax",
    "katex",
    "highlight",
    "toc",
    "hide-published-with",
    "livereload",
    "advanced-emoji",
    "edit-link",
    "footer",
    "language-picker",
    "fontsettings",
    "summary"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "your-disqus-shortname"
    },
    "github": {
      "url": "https://github.com/Julieisbaka/Julieisbaka.github.io"
    },
    "pwa": {
      "cacheId": "mathwiki",
      "runtimeCaching": [
        {
          "urlPattern": "/*",
          "handler": "networkFirst"
        }
      ]
    },
    "edit-link": {
      "base": "https://github.com/Julieisbaka/Julieisbaka.github.io/edit/main/docs/",
      "label": "Edit this page"
    },
    "footer": {
      "copyright": "Â© 2025 Casper Schorr",
      "markdown": true
    },
    "language-picker": {
      "languages": [
        {
          "label": "English",
          "lang": "en"
        },
        {
          "label": "Spanish",
          "lang": "es"
        }
      ]
    }
  }
}